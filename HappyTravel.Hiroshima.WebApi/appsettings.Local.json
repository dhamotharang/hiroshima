{
  "Authority": {
    "Options": "hiroshima/authority",
    "ApiName": "hiroshima",
    "Endpoint": "https://identity-dev.happytravel.com/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug",
      "Microsoft.AspNetCore.Mvc.Internal.ControllerActionInvoker": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "hiroshima",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "DirectContracts": {
    "Database": {
      "ConnectionString": "Server={0};Port={1};Database={2};Userid={3};Password={4};",
      "ConnectionOptions": "hiroshima/connection-strings/direct-contracts"
    }
  }
}
